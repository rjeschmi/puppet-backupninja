<%= 'user = ' + @user if @user %>
<%= 'dbusername = ' + @dbusername if @dbusername %>
<%= 'dbpassword = ' + @dbpassword if @dbpassword %>
<%= 'dbhost = ' + @dbhost if @dbhost %>
<%= 'databases = ' + @databases if @databases %>
<%= 'backupdir = ' + @backupdir if @backupdir %>
<%= 'vsname = ' + @vsname if @vsname %>

hotcopy = <%= @real_hotcopy ? 'yes' : 'no' %>
sqldump = <%= @real_sqldump ? 'yes' : 'no' %>
compress = <%= @real_compress ? 'yes' : 'no' %>
sqldumpoptions = <%= @sqldumpoptions %>

<% if @configfile %>
configfile = <%= @configfile %>
<% end %>

